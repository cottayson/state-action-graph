{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": "./src",
    "module": "es6",
    "target": "ES2020", // "es5"
    "allowJs": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": false
  }
}